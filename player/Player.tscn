[sub_resource type="GDScript" id="GDScript_mr14b"]
script/source = "extends Label

var _frames := 0
var _time := 0.0

func _process(delta: float) -> void:
	_time += delta
	_frames += 1
	if _time > 1.0:
		var s := get_viewport_rect().size
		print(\"Screen is %dx%d\" % [s.x, s.y])
		text = \"FPS: %3d (%.8f mspf)\" % [_frames, _time / _frames]
		_time -= 1.0
		_frames = 0
"

